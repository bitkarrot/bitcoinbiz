<body>
	<div class="error404">
		<div class="error404__body">
			<div class="ec-size ec-size--xl">
				<div class="ec-store">
					<h1 class="error404__title ec-header-h1">404: Page not found</h1>
					<div class="error404__description">You might have followed an outdated link or the page you were looking for no longer exists.</div>
					<div class="form-control form-control--button form-control--primary form-control--large">
						<a class="form-control__button" href="https://www.bramleigh.co.za/">
							<span class="form-control__button-text"><span>Return to Homepage</span></span>
						</a>
					</div>
				</div>
			</div>
		</div>
		<div class="error404__footer">
			<div class="ec-size ec-size--xl">
				<div class="ec-store">
					<form method="get" action="https://www.bramleigh.co.za/search?keyword=">
						<div class="form-control form-control--flexible form-control--fieldset form-control--prepend form-control--large">
							<div class="form-control__loader"></div>
							<input class="form-control__text" type="search" name="keyword" value=""/>
							<div class="form-control__placeholder"><div class="form-control__placeholder-inner">Search for products</div></div>
							<button class="form-control__ico-btn ec-text-muted" type="submit">
								<span class="svg-icon"><svg width="24" height="24" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><g fill="none" fill-rule="evenodd"><path d="M14.335 17.528c4.357 0 7.89-3.532 7.89-7.89 0-4.357-3.533-7.89-7.89-7.89-4.358 0-7.89 3.533-7.89 7.89 0 4.358 3.532 7.89 7.89 7.89zm0 1c-4.91 0-8.89-3.98-8.89-8.89s3.98-8.89 8.89-8.89 8.89 3.98 8.89 8.89-3.98 8.89-8.89 8.89z" fill="currentColor"></path><path d="M7.537 15.292L.684 22.146c-.39.39-.39 1.023 0 1.414.39.39 1.023.39 1.414 0l6.853-6.854c.392-.39.392-1.023 0-1.414-.39-.39-1.022-.39-1.413 0z" fill="currentColor"></path></g></svg></span>
							</button>
						</div>
					</form>
				</div>
			</div>
			<div class="error404__links">
				<a class="error404__link" href="https://www.bramleigh.co.za/">
Homepage				</a><a class="error404__link" href="https://www.bramleigh.co.za/cart">
Shopping cart				</a><a class="error404__link" href="https://www.bramleigh.co.za/account/settings">
My account				</a>
			</div>
		</div>
	</div>
	<script>
		document.querySelector('form').addEventListener('submit', function(e) {
			e.preventDefault();
			document.location = "https://www.bramleigh.co.za/search?keyword=" + encodeURIComponent(input.value);
		});

		var input = document.querySelector('.form-control__text');
		input.addEventListener('input', function() {
			this.parentNode.classList.toggle('form-control--empty', !this.value);
		});

		input.addEventListener('focus', function() {
			this.parentNode.classList.add('form-control--focus');
		});

		input.addEventListener('blur', function() {
			this.parentNode.classList.remove('form-control--focus');
		});

		window.addEventListener("pageshow", () => {
			input.parentNode.classList.toggle('form-control--empty', !input.value);
		});
	</script>
</body>
